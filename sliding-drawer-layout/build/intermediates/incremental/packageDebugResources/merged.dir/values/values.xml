<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingDrawerLayout">
        <attr format="float|fraction" name="widthPercent_leftDrawer">
            <enum name="unspecified" value="0"/>
        </attr>
        <attr format="float|fraction" name="widthPercent_rightDrawer">
            <enum name="unspecified" value="0"/>
        </attr>
        <attr format="float|fraction" name="widthPercent_startDrawer">
            <enum name="unspecified" value="0"/>
        </attr>
        <attr format="float|fraction" name="widthPercent_endDrawer">
            <enum name="unspecified" value="0"/>
        </attr>

        <attr format="boolean" name="enabledInTouch_leftDrawer"/>
        <attr format="boolean" name="enabledInTouch_rightDrawer"/>
        <attr format="boolean" name="enabledInTouch_startDrawer"/>
        <attr format="boolean" name="enabledInTouch_endDrawer"/>

        <attr format="dimension" name="contentSensitiveEdgeSize"/>

        <attr format="color" name="contentFadeColor"/>

        <attr format="integer" name="duration"/>

        <!--<attr name="closeOpenDrawerOnBackPressedEnabled" format="boolean" />-->
    </declare-styleable>
    <declare-styleable name="SlidingDrawerLayout_Layout">
        <attr name="android:layout_gravity"/>
    </declare-styleable>
</resources>